    font-family: &quot;Space Grotesk&quot;; /* Set font family */
    border: 1px solid white; /* Set border color for the entire table */
    gridline-color: transparent; /* Remove default grid lines */
}

/* Apply styles to the header items in the QTableWidget */
QHeaderView::section {
    background-color: #7A798E; /* Set background color for header */
    color: white; /* Set text color for header */
    padding: 2px; /* Adjust padding as needed */
    border: 1px solid white; /* Set border color for header */
}

/* Apply styles to the items (cells) in the QTableWidget */
QTableView::item {
    padding: 2px; /* Adjust padding as needed */
}

/* Apply styles to horizontal grid lines */
QTableView::item {
    border-top: 1px solid white; /* Set top border color for horizontal lines */
}

/* Apply styles to the top-left corner */
QTableWidget::corner-button {
    border-top: 1px solid white; /* Set top border for the top-left corner */
    border-left: 1px solid white; /* Set left border for the top-left corner */
}

/* Apply font styles */
QTableWidget::item, QHeaderView::section {
